{
  "name": "vue-css-loading",
  "description": "A Vue plugin for loading with css animation",
  "version": "1.0.8",
  "author": "Flying <riafan@hotmail.com>",
  "license": "MIT",
  "private": false,
  "main": "./lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot --config build/webpack.conf.js",
    "build": "cross-env NODE_ENV=production webpack --hide-modules --config build/webpack.conf.js",
    "build-umd": "cross-env NODE_ENV=production webpack --config build/webpack.umd.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szriafan/vue-css-loading.git"
  },
  "dependencies": {
    "vue": "^2.5.11"
  },
  "keywords": [
    "vue",
    "plugin",
    "css"
  ],
  "homepage": "https://github.com/szriafan/vue-css-loading",
  "bundleDependencies": false,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.3.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "sass": "^1.32.8",
    "sass-loader": "^7.3.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.11",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  }
}
